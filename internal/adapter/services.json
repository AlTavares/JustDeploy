[
  {
    "icon": "https://upload.wikimedia.org/wikipedia/commons/thumb/2/29/Postgresql_elephant.svg/640px-Postgresql_elephant.svg.png",
    "name": "Postgres",
    "image": "postgres:latest",
    "volume": "/var/lib/postgresql/data",
    "ports": ["5432"],
    "envs": ["POSTGRES_USER", "POSTGRES_DB"],
    "secrets": ["POSTGRES_PASSWORD"],
    "connect_url": "postgres://POSTGRES_USER:POSTGRES_PASSWORD@localhost:5432/POSTGRES_DB"
  },
  {
    "icon": "https://grafikart.fr/uploads/icons/redis.svg",
    "name": "Redis",
    "image": "redis:latest",
    "ports": ["6379"],
    "envs": [],
    "secrets": ["REDIS_PASSWORD"],
    "volume": "/data"
  },
  {
    "icon": "https://upload.wikimedia.org/wikipedia/fr/thumb/4/45/MongoDB-Logo.svg/1280px-MongoDB-Logo.svg.png",
    "name": "mongo",
    "image": "mongo:latest",
    "ports": ["27017"],
    "envs": ["MONGO_INITDB_ROOT_USERNAME"],
    "secrets": ["MONGO_INITDB_ROOT_PASSWORD"],
    "volume": "/data/db"
  },
  {
    "icon": "https://upload.wikimedia.org/wikipedia/fr/thumb/6/62/MySQL.svg/1200px-MySQL.svg.png",
    "name": "mysql",
    "image": "mysql:latest",
    "ports": ["3306"],
    "envs": ["MYSQL_DATABASE"],
    "secrets": ["MYSQL_ROOT_PASSWORD"],
    "volume": "/var/lib/mysql"
  },
  {
    "icon": "https://www.keycloak.org/resources/images/logo.svg",
    "name": "keycloak",
    "image": "jboss/keycloak",
    "ports": ["8080"],
    "envs": ["KEYCLOAK_USER"],
    "secrets": ["KEYCLOAK_PASSWORD"],
    "volume": "/opt/jboss/keycloak/standalone/data"
  }
]
